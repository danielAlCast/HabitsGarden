<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="mx.cdac.habitsgarden">

    <uses-permission android:name="android.permission.FOREGROUND_SERVICE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/gg_iconos_2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/gg_iconos_2_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HabitsGarden">

        <activity android:name=".Recompensas" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PopActivity"
            android:theme="@style/PopMe" />
        <activity
            android:name=".PopInfBtnesActivity"
            android:theme="@style/PopMe" />
        <activity android:name=".CalendarioActivity" />

        <activity
            android:name=".PopActivityRosas"
            android:theme="@style/PopMe" />
        <activity
            android:name=".PopPomodorobtn"
            android:theme="@style/PopMe" />

        <activity
            android:name=".Habitos"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity android:name=".InsertActividad" />
        <activity android:name=".consultar" />
        <activity android:name=".PomodoroActi" />
        <activity
            android:name=".PromoActivity"
            android:theme="@style/PopMe" />

        <service android:name=".Servicio" />
        <service android:name=".ServicioP" />
        <service android:name=".ServicioIntent" />

        <activity android:name=".ActivityTimer" />
    </application>

</manifest>